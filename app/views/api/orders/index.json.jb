@orders.map do |order|
  {
    id: order.id,
    user_id: order.user_id,
    journey_id: order.journey_id,
    journey: {
      id: order.journey.id,
      name: order.journey.name,
      price: order.journey.price,
      images: order.journey.images,
    },
    review_rating: order.review_rating,
    review_comment: order.review_comment,
    subtotal: order.subtotal,
    tax: order.tax,
    total: order.total,
  }
end
